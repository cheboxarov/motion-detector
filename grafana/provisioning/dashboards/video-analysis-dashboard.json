      {
        "id": 4,
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "type": "stat",
        "title": "Процент успешных обработок",
        "targets": [
          {
            "expr": "sum(rate(video_processed_total{status=\"completed\"}[5m])) / sum(rate(video_processed_total[5m])) * 100",
            "refId": "A",
            "datasource": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 80,
                  "color": "yellow"
                },
                {
                  "value": 95,
                  "color": "green"
                }
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
